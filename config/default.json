{
  "settings": {
    "loggerFormat": "tiny",
    "session": {
      "store": "@foal/typeorm"
    }
  },
  "database": {
    "type": "mariadb",
    "database": "env(DATABASE_NAME)",
    "host": "env(DATABASE_HOST)",
    "port": 3306,
    "username": "env(DATABASE_USER_NAME)",
    "password": "env(DATABASE_PASSWORD)"
  }
}
